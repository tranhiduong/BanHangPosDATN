<Window x:Class="BanHangDATN.MoMoQR"
        
         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BanHangDATN"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="Invoice" Height="400" Width="400" ResizeMode="NoResize" WindowStyle="None" FontFamily="Roboto" WindowStartupLocation="CenterScreen">
    <StackPanel>

        <Image x:Name="pic_qrcode" Width="300" Height="300" Margin="0,30,0,0">
            
        </Image>
        <StackPanel Orientation="Horizontal">
            <Button Content="Thành Công" Name="btnThanhCong" Click="btnThanhCong_Click" Width="150" FontSize="20" Margin="20,10,0,0"/>
            <Button Content="Hủy" Name="btnHuy" Click="btnHuy_Click" FontSize="20" Margin="60,10,0,0" Width="150"/>
        </StackPanel>
        
    </StackPanel>
</Window>